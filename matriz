//fa√ßa um vetor com matriz 20 e mostre na tela as 20 matrizes
#include <iostream>
using namespace std;

int main()
{
   int matriz[20], cont;

   for(cont=0 ; cont<20 ; cont++)
       matriz[cont] = cont+1;

   for(cont=0 ; cont<20 ; cont++)
       cout << "Matriz " << cont+1 << ": " << matriz[cont] << endl;
}
